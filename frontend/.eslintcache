, reversed, showSelectableGroup, selectedItems, selectingItems } = this.state\n    const itemsToRender = disableFirstRow ? items.slice(5) : items\n    const orderedItems = reversed ? itemsToRender.slice().reverse() : itemsToRender\n    // console.log(\"this.countersRef.current\", this.countersRef.current)\n    return (\n      <div>\n        {/* <Counters selectedItems={selectedItems} selectingItems={selectingItems} /> */}\n        <div className=\"timeTable\">\n          <Button variant=\"outlined\" className=\"selectable-btn\" onClick={this.toggleSelectableGroup}>課堂時段</Button>\n          <SelectableGroup\n              ref={this.getSelectableGroupRef}\n              className=\"selectable-main\"\n              clickClassName=\"selectable-tick\"\n              enableDeselect={true}\n              tolerance={0}\n              deselectOnEsc={true}\n              allowClickWithoutSelected={false}\n              \n              duringSelection={this.handleSelecting}\n              onSelectionFinish={this.handleSelectionFinish}\n              ignoreList={['.not-selectable']}\n            >\n              <List items={orderedItems} showSelectableGroup={showSelectableGroup} toggleSelectableGroup={this.toggleSelectableGroup}/>\n          </SelectableGroup> \n        </div>\n        {/* <p className=\"not-selectable\">Press ESC to clear selection</p> */}\n      </div>\n    )\n  }\n}\n\nexport default App\n",["263","264"],"/Users/tong/GitHub/web_programming/Final/Web-Final/frontend/src/containers/Query/components/TimeTable/List.tsx",["265"],"import React, { memo } from 'react'\n\nimport { DeselectAll, SelectAll } from 'react-selectable-fast'\nimport { Card } from './Card'\n\ntype TTimeItem = {\n  day: string,\n  section: string\n}\n\ntype TListProps = {\n  showSelectableGroup: boolean,\n  toggleSelectableGroup: () => void,\n  items: TTimeItem[]\n}\nexport const List = memo((props: TListProps) => {\n  const { items, showSelectableGroup, toggleSelectableGroup } = props\n\n  return (\n    <>\n      <div className=\"selectable-button-container\">\n        {showSelectableGroup && (\n          <>\n          <SelectAll component=\"button\" type=\"button\" className=\"selectable-btn\">\n              Select all\n          </SelectAll>\n          <DeselectAll component=\"button\" type=\"button\" className=\"selectable-btn\">\n              Clear selection\n          </DeselectAll>\n        \n          <div className=\"selectable-table\">\n            {items.map(item => (\n              <Card key={`${item.day}-${item.section}`} day={item.day} section={item.section}/>\n            ))}\n          </div>\n        </>)}\n      </div>\n    </>\n  )\n})\n","/Users/tong/GitHub/web_programming/Final/Web-Final/frontend/src/containers/Query/components/TimeTable/Card.tsx",["266","267","268"],"import React from 'react'\nimport { createSelectable, TSelectableItemProps } from 'react-selectable-fast'\n\ntype TAlbumProps = {\n  player: string\n  year: number\n}\ntype TItemsProps = {\n  day: string,\n  section: string\n}\n\nconst X = 'X';\n\nexport const Card = createSelectable<TItemsProps>((props: TSelectableItemProps & TItemsProps) => {\n  const { selectableRef, isSelected, isSelecting, day, section } = props\n\n  const classNames = [\n    'selectable-item',\n    [day, section].includes(X) && 'not-selectable',\n    // DISABLED_CARD_YEARS.includes(year) && 'not-selectable',\n    isSelecting && 'selecting',\n    isSelected && 'selected',\n  ]\n    .filter(Boolean)\n    .join(' ')\n\n  return (\n    <div ref={selectableRef} className={classNames}>\n      {\n        classNames.includes('not-selectable') && <h2>{day!=X && day}{section!=X && section}</h2>\n      }\n    </div>\n  )\n})\n","/Users/tong/GitHub/web_programming/Final/Web-Final/frontend/src/containers/Header/components/ntuLogo.js",["269"],"/Users/tong/GitHub/web_programming/Final/Web-Final/frontend/src/containers/Header/components/breadcrumbs.js",["270","271"],"import React from 'react';\nimport { Breadcru